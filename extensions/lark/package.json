{
  "name": "@openclaw/lark",
  "version": "2026.1.30",
  "description": "OpenClaw Lark/Feishu channel plugin",
  "type": "module",
  "dependencies": {
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "openclaw": "workspace:*"
  },
  "openclaw": {
    "extensions": ["./index.ts"],
    "channel": {
      "id": "lark",
      "label": "Lark/Feishu",
      "selectionLabel": "Lark/Feishu (plugin)",
      "docsPath": "/channels/lark",
      "docsLabel": "lark",
      "blurb": "Lark/Feishu custom bot webhook channel; install the plugin to enable.",
      "order": 70
    },
    "install": {
      "npmSpec": "@openclaw/lark",
      "localPath": "extensions/lark",
      "defaultChoice": "local"
    }
  }
}
